<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig
	PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="false" errorTracingEnabled="true" maxRequests="32"
		maxSessions="10" maxTransactions="5" useStatementNamespaces="true" />
	<sqlMap resource="persist/ibatis-presence-sqlmap.xml"/>
	<sqlMap resource="persist/ibatis-resource-sqlmap.xml"/>
	<sqlMap resource="persist/ibatis-subscribe-sqlmap.xml"/>
	<sqlMap resource="persist/ibatis-common-sqlmap.xml"/>
	<sqlMap resource="persist/ibatis-subject-sqlmap.xml"/>
</sqlMapConfig>